<?xml version="1.0" encoding="utf-8" ?>
<?xml-stylesheet type="text/xsl" href="/XSLTransform/LSTM.xsl" ?>
<root>
  <input>
    <inputCSV>
        <file>
            <name>{filename}</name>
            <url>{filename}</url> 
        </file>
    </inputCSV>
    <look_back>{look_back}</look_back>
    <start_index>{start_index}</start_index>
    <end_index>{end_index}</end_index>
    <normalization>{normalization}</normalization>
    <formation>{formation}</formation>
    <epochs>{epochs}</epochs>
    <batch_size>{batch_size}</batch_size>
    <expand_num>{expand_num}</expand_num>
  </input>
  <output>
    <result>
      <file>
        <name>result.tar.gz</name>
        <url>result.tar.gz</url>
      </file>
    </result>
  </output>
</root>